<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <RootNamespace>Suave.Minify</RootNamespace>
    <AssemblyName>Suave.Minify</AssemblyName>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <Authors>Ademar Gonzalez</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Suave.Minify</PackageId>
    <Version>2.1.0</Version>
    <Description>A minify and bundling WebPart for Suave</Description>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Library.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FSharp.Core" Version="10.0.101" />
    <PackageReference Include="Suave" Version="3.2.2" />
    <PackageReference Include="YUICompressor.NET" Version="3.1.0" />
    <PackageReference Include="EcmaScript.Net" Version="2.0.0" />
  </ItemGroup>

</Project>